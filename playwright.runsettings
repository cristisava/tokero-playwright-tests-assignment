<RunSettings>
	<MSTest>
		<Parallelize>
			<Workers>1</Workers>
			<Scope>ClassLevel</Scope>
		</Parallelize>
	</MSTest>
	<RunConfiguration>
		<EnvironmentVariables>
			<DEBUG>pw:api</DEBUG>
		</EnvironmentVariables>
	</RunConfiguration>
	<Playwright>
		<BrowserName>chromium</BrowserName>
		<ExpectTimeout>5000</ExpectTimeout>
		<LaunchOptions>
			<Headless>false</Headless>
			<Channel>chrome</Channel>
		</LaunchOptions>
	</Playwright>
</RunSettings>